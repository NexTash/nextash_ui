 "html": "<div class=\"intro\">\n    <div class=\"table1\">\n    <table>\n            <tr>\n               <td> <strong>CLIENT</strong>  </td> \n               <td>  {{doc.customer_name}}   </td>\n           </tr>\n            <tr>\n               <td> <strong>PROJECT NAME</strong>  </td> \n               <td>  {{doc.project}}   </td>\n           </tr>\n           <tr>\n               <td> <strong>TERRITORY</strong>  </td> \n               <td>  {{doc.territory}}   </td>\n           </tr>\n            \n        </table>\n        </div>\n        <div class=\"table2\">\n         <table>\n            <tr>\n               <td> <strong>DATE</strong>  </td> \n               <td>  {{doc.posting_date}}   </td>\n           </tr>\n            <tr>\n               <td> <strong>QTN</strong>  </td> \n               <td>  {{doc.name}}   </td>\n           </tr>\n           <tr>\n               <td> <strong>CONTRACT</strong>  </td> \n               <td>  {{doc.name}}   </td>\n           </tr>\n            \n        </table></div>\n        </div>\n\n\n\n<div class=\"items\">\n    <table class=\"table table-bordered\">\n      <thead>\n        <tr >\n          <th width= 5%>ITEM</th>\n          <th width= 35% >DESCRIPTION</th>\n          \n          <th width= 10%>UNIT</th>\n          <th width= 10%>QTY</th>\n          <th width= 20%>UNIT PRICE</th>\n          <th width= 20%>AMOUNT</th>\n        </tr>\n        <tr>\n            <th colspan=\"6\">DESIGN</th>\n        </tr>\n      </thead>\n      <tbody>\n        {%- for row in doc.items -%}\n        <tr>\n          <td>{{ row.idx }}</td>\n          <td>{{ row.description}}</td>\n          <td>{{ row.qty}}</td>\n          <td>{{ row.qty}}</td>\n          <td>{{row.get_formatted(\"rate\", doc) }}</td>\n          <th >{{row.get_formatted(\"amount\", doc) }}</th>\n        </tr>\n        {%- endfor -%}\n        <tr>\n            <td colspan=\"4\" style=\"text-align:left;border-color: transparent;\" ><i>Final Price:{{doc.in_words}}</i></td>\